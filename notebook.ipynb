 "cells": [
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# HDI Analysis with Apriori Algorithm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Install and load necessary libraries\n",
    "install.packages(\"arules\")\n", "install.packages(\"readr\")\n"
    "library(arules)\n",
    "library(readr)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load dataset\n",
    "hdi_data <- read_csv(\"human-development-index-hdi-2014.csv\")\n",
    "\n",
    "# View the dataset structure (optional)\n",
    "str(hdi_data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Convert data to transactions ",
    "trans <- as(hdi_data, \"transactions\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Apply the Apriori algorithm\n",
    "rules <- apriori(trans, parameter = list(supp = 0.01, conf = 0.5))\n",
    "\n",
    "# Inspect the rules\n",
    "inspect(head(rules,4))"
   ]
  }
 ],
